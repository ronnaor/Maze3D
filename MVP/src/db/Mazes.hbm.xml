<?xml version="1.0"?>
  <!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
  
  <hibernate-mapping package="db">
  
  <class name="SimpelingMaze" table="mazes">
        <id name="mazeID" type="int" column="maze_id">
         <generator class="native"/>
      </id>
    <property name="simpleMaze" type="blob" length="1000000000" column="maze" />
       <one-to-one name="DBObject" class="db.DBObject" constrained="true">
     </one-to-one>
   </class>
     </hibernate-mapping>