<?xml version="1.0"?>
  <!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
  
    <hibernate-mapping >
  <class name="SimpelingMaze" table="mazes" catalog="DB">
        <id name="mazeID" type="int">
			<column name="maze_id" />
			<generator class="foreign">
				<param name="property">All</param>
			</generator>
		</id>
    <property name="simpleMaze" type="blob" length="1000000000" column="maze" />
       <one-to-one name="DBObject" class="DBObject" constrained="true">
     </one-to-one>
   </class>
     </hibernate-mapping>