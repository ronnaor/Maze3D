<?xml version="1.0"?>
  <!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
  
  <hibernate-mapping package="db">
      <class name="DBObject" table="All" >
          <id name="mazeID" type="int" column="maze_id" >
                <generator class="identity" />
          </id>
       	  <one-to-one name="mazes" class="db.Mazes"  cascade="save-update">
       	  </one-to-one>
            <one-to-one name="solutions" class="db.Solutions" cascade="save-update">
            </one-to-one>       	 
       	 <property name="name" type="string">
            <column name="name" length="20" not-null="true" unique="true" />
        </property>
      </class>    
  </hibernate-mapping>

  