<?xml version="1.0"?>
  <!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
  
  <hibernate-mapping >
  <class name="db.DBObject" table="All" catalog="DB">
        <id name="mazeID" type="int">
            <column name="maze_id" />
            <generator class="identity" />
        </id>
                 	 <property name="name" type="string">
            <column name="name" length="20" not-null="true" unique="true" />
        </property>
       	  <one-to-one name="maze" class="db.SimpelingMaze"  cascade="save-update">
      	 </one-to-one>
            <one-to-one name="solution" class="db.Solutions" cascade="save-update">
            </one-to-one> 
      </class>    
  </hibernate-mapping>

  