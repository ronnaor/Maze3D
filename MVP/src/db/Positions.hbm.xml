<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  
<class name="Positions" table="Positions" 
		catalog="DB">
        <id name="mazeID" type="int">
            <column name="num" />
            <generator class="identity" />
        </id>
      <many-to-one name="Solutions" class="Solutions" fetch="select">
            <column name="mazeID" not-null="true" />
        </many-to-one>
      <property name="x" column="x" type="int"/>
      <property name="y" column="y" type="int"/>
      <property name="z" column="z" type="int"/>
    </class>
</hibernate-mapping>