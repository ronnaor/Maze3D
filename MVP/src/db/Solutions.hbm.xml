<?xml version="1.0"?>
  <!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
  
    <hibernate-mapping >
  <class name="db.Solutions" table="Solutions" catalog="DB">
        <id name="mazeID" type="int">
			<column name="maze_id" />
			<generator class="foreign">
				<param name="property">All</param>
			</generator>
		</id>
      <property name="numMoves" column="numMoves" type="int"/>
      <list name="path"  cascade="all">
         <key column="posID"/>
         <list-index column="idx"/>
         <one-to-many class="db.Positions"/>
      </list>
      <one-to-one name="obj" class="db.DBObject" constrained="true">
        </one-to-one>
   </class>
     </hibernate-mapping>